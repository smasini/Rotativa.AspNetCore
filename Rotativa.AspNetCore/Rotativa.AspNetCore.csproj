<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.7.1</Version>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <RepositoryUrl>https://github.com/smasini/Rotativa.AspNetCore</RepositoryUrl>
    <PackageProjectUrl>https://github.com/smasini/Rotativa.AspNetCore</PackageProjectUrl>
    <Company>Mayp Digital SRL</Company>
    <Authors>Simone Masini</Authors>
    <Description>Rotativa: PDF easy creation for Asp.Net Core</Description>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageTags>PDF AspNetCore</PackageTags>
    <Copyright></Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.0.1" />
  </ItemGroup>
</Project>
